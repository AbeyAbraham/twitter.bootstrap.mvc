<?xml version="1.0"?>
<package>
  <metadata>
    <id>twitter.bootstrap.mvc4</id>
    <version>1.0.0</version>
    <authors>Eric Hexter, Matt Hinze</authors>
    <description>This has the bootstrap layout and supporting files to quickly add Twitter Bootstrap to an MVC 4 application</description>
	  <dependencies>
      <dependency id="html5shiv" />
	<dependency id="twitter.bootstrap" />
      	<dependency id="navigationroutes.mvc4" />
	<dependency id="microsoft.web.optimization" version="[1.0.0-beta2]"/>
      	<dependency id="jquery" />
      	<dependency id="Microsoft.jQuery.Unobtrusive.Ajax" />
      	<dependency id="Microsoft.jQuery.Unobtrusive.Validation" />
    </dependencies> 
  </metadata>
  <files>
    <file src="readme.txt" target="readme.txt"></file>
    <file src="App_Start\*.*" target="Content\App_Start"></file>
    <file src="BootstrapSupport\*.*" target="Content\BootstrapSupport"></file>
    <file src="Views\Shared\_BootstrapLayout.cshtml.pp" target="Content\Views\Shared"></file>
    <file src="Views\_ViewStart.cshtml" target="Content\Views"></file>
    <file src="Scripts\*.*" target="Content\Scripts"></file>
  </files>
</package>